{
	"compileOnSave": true,
	"compilerOptions": {
		"importHelpers": true,
		"incremental": true,
		"lib": [
			"ESNext",
			"DOM"
		],
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"newLine": "lf",
		"noEmitHelpers": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"pretty": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"strict": true,
		"target": "ESNext",
		"useDefineForClassFields": true,
		"rootDir": "src",
		"outDir": "dist",
		"baseUrl": "src",
		"paths": {
			"@base": [
				"./index.js"
			],
			"@buttons/*": [
				"buttons/*"
			],
			"@commands/*": [
				"commands/*"
			],
			"@lib/*": [
				"lib/*"
			]
		}
	}
}