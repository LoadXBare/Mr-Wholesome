generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model guildConfig {
  guildId         String @id
  logChannel      String @default("")
  ignoredChannels String @default("[]")
}

model guildWarnings {
  warningId        String @id
  guildId          String
  warnerId         String
  warneeId         String
  reason           String @default("None provided.")
  warningTimestamp Int
}
